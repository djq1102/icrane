<div class="span12" >
	<!-- BEGIN EXAMPLE TABLE PORTLET-->
	<div class="portlet box grey">
		<div class="portlet-title">
			<div class="caption"><i class="icon-user"></i>$searchListTitle</div>
			<div class="actions">
				<a href="toAdd" class="btn blue"><i class="icon-pencil"></i> 新增</a>
			</div>
		</div>

		<div class="portlet-body">
			<table class="table table-striped table-bordered table-hover" id="sample_2">
				<thead>
					<tr>
                        #foreach($searchTitle in $searchTitleList)
        					#fillSearchListTableTitle($searchTitle)
        				#end
					</tr>
				</thead>
				<tbody role="alert" aria-live="polite" aria-relevant="all">
				</tbody>
			</table>
		</div>
	</div>
	<!-- END EXAMPLE TABLE PORTLET-->
</div>